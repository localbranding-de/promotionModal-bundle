{
	"name" : "localbranding-de/promotionmodal-bundle",
	"type" : "contao-bundle",
	"description" : "Backend Design for Contao",
	"license" : "LGPL-3.0-or-later",
	"version" : "1.0.0",
	"authors" : [{
			"name" : "Localbranding-de",
			"homepage" : "https://github.com/localbranding-de"
		}
	],
	"require" : {
		"php" : "^5.6 || ^7.0",
		"contao/core-bundle" : "4.4.*",
		"symfony/framework-bundle" : "^3.3"
	},
	"conflict" : {
		"contao/core" : "*",
		"contao/manager-plugin" : "<2.0 || >=3.0"
	},
	"require-dev" : {
		"contao/manager-plugin" : "^2.0",
		"doctrine/doctrine-cache-bundle" : "^1.3",
		"friendsofphp/php-cs-fixer" : "^2.6",
		"leofeyer/optimize-native-functions-fixer" : "^1.1",
		"php-http/guzzle6-adapter" : "^1.1",
		"php-http/message-factory" : "^1.0.2",
		"phpunit/phpunit" : "^5.7.26",
		"symfony/phpunit-bridge" : "^3.2"
	},
	"extra" : {
		"contao-component-dir" : "src/",
		"contao-manager-plugin" : "LocalbrandingDe\\PromotionModalBundle\\ContaoManager\\Plugin"
	},
	"autoload" : {
		"psr-4" : {
			"LocalbrandingDe\\PromotionModalBundle\\" : "src/"
		}
	},
	"support" : {
		"issues" : "https://github.com/localbranding-de/promotionmodal-bundle/issues",
		"source" : "https://github.com/localbranding-de/promotionmodal-bundle"
	}
}
